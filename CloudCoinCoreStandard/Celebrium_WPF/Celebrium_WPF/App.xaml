             xmlns:local="clr-namespace:Celebrium_WPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionaries/YellowButtonDictionary.xaml"/>
